    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "pinia": "^2.1.7",
    "vant": "^4.8.5",
    "vue": "~3.3.13",
    "vue-demi": "^0.14.7",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
